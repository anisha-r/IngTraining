package com.hcl.ecommerce.repository;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;
import com.hcl.ecommerce.entity.StoreReview;


@Repository
public interface StoreReviewRepository extends JpaRepository<StoreReview, Integer>{
    List<StoreReview> findByStoreId(Integer storeId);
    StoreReview findByUserId(Integer userId);
	StoreReview countUserIdByStoreId(Integer storeId);
    
    
}
